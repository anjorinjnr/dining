{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "laravel/framework": "4.2.11",
        "cartalyst/sentry": "2.1.*",
        "symfony/translation": "2.5.5",
        "jenssegers/date": "2.0.8",
        "eventviva/php-image-resize": "1.2.3",
        "guzzlehttp/guzzle": "~5.0",
        "sairiz/mandrill": "1.0.4"
        
        
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "1.*",
        "way/generators": "2.*",
        "doctrine/dbal": "~2.3",
        "phpspec/phpspec": "~2.0",
        "phpunit/phpunit": "4.3.*",
        "behat/behat": "~3.0.6",
        "benconstable/phpspec-laravel": "~1.0",
        "fzaninotto/faker": "1.4.0"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/Dining/Controllers",
            "app/Dining/Models",
            "app/Dining/Repository",
            "app/Dining/Validators",
            "app/Dining/Services",
            "app/Dining/Traits",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "bin-dir": "bin/"
    },
    "minimum-stability": "stable"
}
